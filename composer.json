{
    "name": "creem/laravel",
    "description": "Official Laravel package for CREEM payment integration",
    "keywords": ["laravel", "creem", "payment", "api"],
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "H33T Patel",
            "email": "hitkumarp589@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/http": "^10.0|^11.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^9.16"
    },
    "autoload": {
        "psr-4": {
            "Creem\\Laravel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Creem\\Laravel\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Creem\\Laravel\\CreemServiceProvider"
            ],
            "aliases": {
                "Creem": "Creem\\Laravel\\Facades\\Creem"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
